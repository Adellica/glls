;; glsl.setup -*- scheme -*-

(compile -d0 -O2 -J -s glsl-compiler.scm)
(compile -d0 -O2 -s glsl-compiler.import.scm)
(compile -d0 -O2 -J -s glsl.scm)
(compile -d0 -O2 -s glsl.import.scm)

(install-extension
 'glsl
 '("glsl.so" "glsl.import.so" "glsl-compiler.so" "glsl-compiler.import.so")
 '((version "0.0.1")))
